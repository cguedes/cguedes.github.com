#VRML V2.0 utf8


Viewpoint {
	position 8 1.5 8
}

####################################
# Ambiente (fundo e céu)            
####################################
Background {
    skyColor [
        0.0 0.2 0.7,
        0.0 0.5 1.0,
        1.0 1.0 1.0
    ]
    skyAngle [ 1.309, 1.571 ]
    groundColor [
        0.1 0.10 0.0,
        0.4 0.25 0.2,
        0.6 0.60 0.6,
    ]
    groundAngle [ 1.309, 1.571 ]
}

####################################
#		Porta                       
####################################
PROTO PortaAbreDir []
{
	Group {
		children [
		   Transform {
			translation	0.5 1.125 0
			children  Shape {
				geometry Box { size 1 2.25 0.1 }
				appearance Appearance {
					material Material {	
						diffuseColor .9 .41 0
						emissiveColor .1 .04 0
						ambientIntensity .11
						shininess .08					
					}
				}	
			}
		   }
		   Transform {
			   translation 0.1 1 0.1
			   children	Shape {
					geometry	Sphere { radius 0.05 }
					appearance Appearance { material Material { diffuseColor 0.8 0.8 0.8} }
				}
		   }
		   Transform {
			   translation 0.1 1 -0.1
			   children	Shape {
				   geometry	Sphere { radius 0.05 }
				   appearance Appearance { material	Material { diffuseColor	0.8 0.8 0.8 } }
			   }
		   }
		]
	}				
}

