####################################
#		Braço                       
####################################
PROTO Braco []{
	Group {
		children 
		[
			Transform {
				scale 0.75 1 1
				children 
				[
					Shape {
						geometry Cylinder {
							radius 0.1
							height 0.5
						}
						appearance Appearance { material Material { diffuseColor 0.3 0.3 0.3 } }
					}

					# Ombro
					Transform {
						translation	0 0.25 0
						children Shape {
							geometry Sphere	{ radius 0.1 }
							appearance Appearance { material Material { diffuseColor 0.3 0.3 0.3 } }
						}
					}				
				]
			}

			# Mão
			Transform {
				translation	0 -0.25 0
				children Shape {
					geometry Sphere	{ radius 0.05 }
					appearance Appearance { material Material { diffuseColor 1 .68 .35 } }
				}
			}			
		]
	}
}

####################################
#		Perna                       
####################################
PROTO Perna []{
	Group {
		children 
		[
			Transform {
				scale 1 1 0.85
				children Shape {
					geometry Cylinder {
						radius 0.1
						height 1
					}
					appearance Appearance {	material Material { diffuseColor 0.3 0.3 0.3 } }
				}
			}

			# Sapato
			Transform	{
				translation	0 -0.52 0.05
				scale 0.5 1 1
				children 
				[
					Shape {
						geometry Cylinder {
							height 0.05
							radius 0.15
						}
						appearance Appearance { material Material { diffuseColor .25 .17 .07 } }
					}
					Transform {
						translation	0 0.075 0
						children Shape {
							geometry Cone {
								height 0.1
								bottomRadius 0.15
							}
							appearance Appearance { material Material { diffuseColor .25 .17 .07 } }
						}
					}					
				]
			}
		]
	}
}

####################################
#		Tronco                      
####################################
PROTO Tronco []{
	Group {
		children 
		[
			Transform {
				scale 1 1 0.5
				children Shape {
					geometry Cylinder {
						radius 0.2
						height 0.5
					}
					appearance Appearance { material Material { diffuseColor 0.3 0.3 0.3 } }
				}
			}

			# Camisa
			Transform {
				translation	0 0.001 0.001
				scale 0.5 1 0.5
				children Shape	{
					geometry Cylinder {
						radius 0.2
						height 0.5
					}
				}
			}

			# Cartão - Securitas
			Transform {
				translation	0.08 0.15 0.1
				children Shape {
					geometry Box { size 0.1 0.05 0 }
					appearance Appearance {
						material Material {}
						texture	ImageTexture { url	"imagens/Securitas.jpg" }
					}
				}
			}

			# Gravata
			Transform {
				translation	0 0.22 0.08
				rotation 1 0 0 3.14
				children Shape {
					geometry Cone {
						bottomRadius 0.05
						height 0.05
					}
					appearance Appearance { material Material { diffuseColor .88 .15 .01 } }
				}
			}
			Transform {
				translation	0 0.1 0.1
				scale 1 1 0.5
				children Shape {
					geometry Cone {
						bottomRadius 0.05
						height 0.3
					}
					appearance Appearance { material Material { diffuseColor .88 .15 .01 } }
				}
			}
			Transform {
				translation	0 -0.07 0.1
				rotation 1 0 0 3.14
				scale 1 1 0.5
				children Shape {
					geometry Cone {
						bottomRadius 0.05
						height 0.05
					}
					appearance Appearance { material Material { diffuseColor .88 .15 .01 } }
				}
			}
		]
	}
}

####################################
#		Segurança                   
####################################
PROTO Seguranca []{
	Group {
		children 
		[
			Transform {
				translation	0 0.78 0
				children Cabeca { }
			}

			Transform {
				translation	0 0.35 0
				children Tronco	{ }
			}

			Transform {
				translation	0 0.03 0
				rotation 1 0 0 3.14
				scale 1 1 0.5
				children Shape {
					geometry Cone {
						bottomRadius 0.2
						height 0.15
					}
					appearance Appearance { material Material { diffuseColor 0.3 0.3 0.3 } }
				}
			}

			Transform {
				translation	0.2 0.26 0
				children Braco { }
			}

			Transform {
				translation	-0.2 0.26 0
				children Braco { }
			}

			Transform {
				translation	0.1 -0.4 0
				children Perna { }
			}

			Transform {
				translation	-0.1 -0.4 0
				children Perna { }
			}
	   
		]
	}
}	


