#PORTAS E ARMACOES

PROTO ArmacaoUn[] {
	Transform	{
		rotation 1 0 0 1.57
		children [
		
			Transform {			
    			translation	-0.5 -0.1 -1.05
				children [
				  
					Shape{
					   appearance Appearance {	
						   material Material { 
								diffuseColor .643 .396 .09
						   }
					   }
					   geometry Extrusion {
						  crossSection [0 0, 1 0, 1 2.1 , 0.9 2.1 , 0.9 0.1 , 0.1 0.1 , 0.1 2.1 , 0 2.1 , 0 0 ]
    					  spine [0 0 0, 0 0.2 0]	
						  beginCap TRUE
						  endCap TRUE
						  solid	FALSE
					   }

					}
				]
			}

			#ArmacaoPequena
			Transform {			
    			translation	-0.4 0.025 -0.95
				children [
			  
					Shape{
					   appearance Appearance {	
						   material Material { 
								diffuseColor .643 .396 .09
						   }
					   }
					   geometry Extrusion {
						  crossSection [ 0 0 , 0.8 0 , 0.8 2 , 0.78 2 , 0.78 0.02 , 0.02 0.02 , 0.02 2 , 0 2 , 0 0  ]
    					  spine [0 0 0, 0 0.02 0]	
						  beginCap TRUE
						  endCap TRUE
						  solid	FALSE
					   }

					}
				]
			}
		]
	}
}
 
