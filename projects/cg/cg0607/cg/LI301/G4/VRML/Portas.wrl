#MACANETA

PROTO CurvaMacaneta[] {
	Shape{
	   appearance Appearance {	
		   material Material { 
				diffuseColor .6 .11 .13
		   }
	   }
	   geometry Extrusion {
		  crossSection [1.00		0.00	, 0.97		0.26	, 0.87		0.50	, 0.71		0.71	, 0.50		0.87	, 0.26		0.97	, 0.00		1.00	, -0.26		0.97	, -0.50		0.87	, -0.71		0.71	, -0.87		0.50	, -0.97		0.26	, -1.00		0.00	, -0.97		-0.26	, -0.87		-0.50	, -0.71		-0.71	, -0.50		-0.87	, -0.26		-0.97	, 0.00		-1.00	, 0.26		-0.97	, 0.50		-0.87	, 0.71		-0.71	, 0.87		-0.50	, 0.97		-0.26	, 1.00		0.00 ]
    	  spine [2.99	0	-0.26 , 3.00 	0.00	0.00	, 2.90	0.00	0.78	, 2.60	0.00	1.50	, 2.12	0.00	2.12	, 1.50	0.00	2.60	, 0.78	0.00	2.90	, 0.00	0.00	3.00 , -0.26	0.00	2.99 ]	
		  beginCap TRUE
		  endCap TRUE
		  solid	FALSE
		  creaseAngle .5
	   }
	}
}
PROTO MacanetaUmLado[] {

	Transform {
		scale	0.01 0.01 0.01		
    	translation	-.03 0 0.05
		children [
		  
			CurvaMacaneta {}

			#Cil1 - mais largo
			Transform {
				translation	3 0 -1
				rotation 1 0 0 1.57
				children [
				  Shape {
					geometry Cylinder {
						radius 1.3
						height 2
					}
					appearance	Appearance  {
						material Material {
							diffuseColor .6 .11 .13
						}
					}
				  }
				]
			}

			#Cil2 Cilindro comprido
			Transform {
				rotation 0 1 0 1.57
				translation	-2.5 0 3
				children [
					Transform {
						rotation 1 0 0 1.57
						children [
						  Shape {
							geometry Cylinder {
								radius 1
								height 5
							}
							appearance	Appearance  {
								material Material {
									diffuseColor .6 .11 .13
								}
							}
						  }
						]
					}
				]
			}

			Transform {
				rotation 0 1 0 -1.57
	    		translation -5 0 0
				children[
					CurvaMacaneta {}
				]
			}

			Transform {
				translation	-8 0 -.5
				rotation 1 0 0 1.57
				children [
				  Shape {
					geometry Cylinder {
						radius 1
						height 1
					}
					appearance	Appearance  {
						material Material {
							diffuseColor .6 .11 .13
						}
					}
				  }
				]
			}

		]
	}
}

PROTO Macaneta[] {	##Centro é o centro do cilindro grande
	Group {
		children [

			Transform {
				rotation 0 0 0 0
				children [
					MacanetaUmLado {}
				]
			}

			Transform {
				rotation 1 0 0 3.14
				children [
					MacanetaUmLado {}
				]
			}


			Transform {
				rotation 1 0 0	1.57
				scale 0.01 0.01 0.01
				children [
				  Shape {
					geometry Cylinder {
						radius 3.5
						height 6
					}
					appearance	Appearance  {
						material Material {
							diffuseColor .6 .11 .13
						}

					}
				  }
				]
			}
		]
	}
}

#WC HOMENS

PROTO PortaPrincipalH[]{ #Abre para dentro e para a direita (a entrar)

	Group {
		children [
			Transform {
				scale 1.1 1 1
				children[
		    		Group	{
						children [
		    				ArmacaoUn	{}
							DEF PPH Transform {
								center 0.4 0 -.025
								children Transform {
									translation	0 -0.055 0
									children[
									   PortaUnChao{}
									   #Macaneta
									   Transform {
											translation	-0.33 0 0
											rotation 0 0 1 3.14
											children[
												Macaneta {}
											
										    	DEF	TSMacanetaPPH TouchSensor {
														enabled	TRUE
												}

											]
										}
									]
								}
							}
						]
					}
				]
			}

			DEF	PPHTimer TimeSensor	{
				cycleInterval 3 #segundos - tempo de abrir
				loop FALSE
				startTime 0
				stopTime 0
			}

			DEF	PPHScript Script {
				eventIn	SFFloat	set_fraction
				eventOut SFRotation	value_changed
				field SFBool status FALSE
				field SFFloat rotacao -1.57
				url	"portas.js"
			}
		]
	}
	ROUTE TSMacanetaPPH.touchTime TO PPHTimer.startTime
	ROUTE PPHTimer.fraction_changed	TO PPHScript.set_fraction
	ROUTE PPHScript.value_changed TO PPH.rotation
} #Proto


PROTO PortaWCGrandeH[]{ #Abre para fora e para a direita (a entrar)

	Transform {
		scale 1.1 1 1
		rotation 0 1 0 3.14
		children[
			Group	{
				children [
		    		ArmacaoUn	{}
					DEF PGH Transform {
						center -0.4 0 -0.025	  #Coordenadas trocadas pq a porta é a de origem rodada
						children 
							Transform {
								translation	0 -0.005 0
								children[
								   PortaUnAr{}
								   Transform {
										#rotation	0 0 1 3.14								
										translation	.33 0 0
										children[
											Macaneta {}
											DEF	TSMacanetaPGH TouchSensor {
												enabled	TRUE
											}
										]
								    }#Fim Macaneta
								]
							}#Fim Porta e Macaneta
					} #Fim da Porta Macaneta e Centro
				]
			} #Fim Porta e Armacao
		]
	}  #Fim Porta Escalada

	DEF	PGHTimer TimeSensor	{
		cycleInterval 3 #segundos - tempo de abrir
		loop FALSE
		startTime 0
		stopTime 0
	}

	DEF	PGHScript Script {
		eventIn	SFFloat	set_fraction
		eventOut SFRotation	value_changed
		field SFBool status FALSE
		field SFFloat rotacao 1.57
		url	"portas.js"
	}

	ROUTE TSMacanetaPGH.touchTime TO PGHTimer.startTime
	ROUTE PGHTimer.fraction_changed	TO PGHScript.set_fraction
	ROUTE PGHScript.value_changed TO PGH.rotation
} #Fim do Proto

	 
