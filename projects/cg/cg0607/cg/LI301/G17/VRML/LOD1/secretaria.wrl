PROTO Secretaria [] {
	Transform {
		translation	0 0.375 0
		children [
			Group {
				children [
					Transform {
						translation	0.425 0 -1
						children Mesa1 { }
					}
					Transform {
						translation	0.450 0.375 -1
						children Mesa1_Top { }
					}
					Transform {
						translation	0.85 0.50 -1
						rotation 0 1 0 1.57
						children Metal { }
					}
					Transform {
						children Mesa2 { }
					}
					Transform {
						translation	0 0.375 0
						children Mesa2_Top { }
					}
					Transform {
						translation	-1.25 0 0.425
