PROTO Alcatrao
[
  field MFVec2f crossSection []
]
{
  Shape {
    geometry Extrusion {
      crossSection IS crossSection 
      spine [0 -0.1 0, 0 0 0]
    }
    appearance Appearance { material Material { diffuseColor 0 0 0 } }
  }
}

