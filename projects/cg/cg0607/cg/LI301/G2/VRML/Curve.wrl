#VRML V2.0 utf8

PROTO Curve[
	field MFVec2f cross []
	field MFVec3f spi []
]{
	Transform {
		children Shape {
			appearance Appearance {material	Material { diffuseColor	1 1 1 }}
			geometry Extrusion {
				crossSection IS cross
				spine IS spi
				creaseAngle	1
				solid FALSE
			}
		}
	}
}

Curve {}