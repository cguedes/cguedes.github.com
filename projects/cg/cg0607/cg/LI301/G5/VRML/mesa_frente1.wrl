#width = 33, height = 47//mudar, depth = 47
#segue norma HMN
PROTO Bide[]
{
	Group{
		children [
			#placa de encosto
			Transform {
				translation	0 34 0
				children Placa_Pia {}
			}
			
			#Pia
			Transform {
				translation	2 34 0
				children Bide_Pia {}
			}

			#Cano adptador
			Transform {
				translation	7.5 27 16.5
				children Cano_Adptador {}
			}

			#Cano T
			Transform	{
				translation	10 19.5 23.75
				scale 1.14 1 1.29
				rotation 0 1 0 3.14
				children Transform{
					translation	-3.75 -7.5 -3.5 
					#width = 7.5
					children  Cano_T { spine_cano_t [0 -7.5 0, 0 7.5 0, 0 0 0,4 0 0]}
				}
			}
		
			#Cano de curva
			Transform {
				translation	3 18 23.8
				scale 1.5 1.5 1.5 
				#rotation 0 1 0 3.14

				children Transform {				
					translation	-2 -2.5 -1
					children Canto_Cano	{ cor_canto_cano  0.5 0.5 0.5 }
				}
			}

			#cano final
			Transform {
				translation 1.5 7.5 23.8	
				scale 1.5 1.5 1.5
			   
				children Transform {
					translation	-1 -4.5 -1
					children Cano { height [0 0 0 ,0 9 0], cor_canos 0.5 0.5 0.5}
				}
			}
		]
	}
} 

