	  }

	  Transform	{
		  translation 900 2 234
		  children Bancos {}
	  }
	  Transform	{
		  translation 1034 2 234
		  children Bancos {}
	  }

	  Transform	{
		  translation 233.5 2 220
		  children
			  Transform	{
				  rotation 0 1 0 -1.57
				 # translation	 0 0 -453.5 
				  children Mesa_F1 {}
			  }
		}
		 Transform	{
		  translation 233 2 446
		  children
			  Transform	{
				  rotation 0 1 0 1.57				 
				  children Mesa_F2 {}
			  }
		}
		Transform	{
		  translation 233 2 558.5
		  children
			  Transform	{
				  rotation 0 1 0 1.57 				  
				  children Mesa_F2 {}
			  }
		}
		Transform	{
		  translation 505 2 181
		  children
			  Transform	{
				  rotation 0 1 0 -1.57
				 # translation	 0 0 -453.5 
				  children Mesa_Lab {}
			  }
		}

		Transform	{
		  translation 767 2 181
		  children
			  Transform	{
				  rotation 0 1 0 -1.57
				 # translation	 0 0 -453.5 
				  children Mesa_Lab {}
			  }
		}

			Transform	{
		  translation 1029 2 181
		  children
			  Transform	{
				  rotation 0 1 0 -1.57
				 # translation	 0 0 -453.5 
				  children Mesa_Lab {}
			  }
		}

	   Transform	{
		  translation 233 2 149
		  children Chuveiro {}
	  }



	   Transform	{
		  translation 472 2 18
		  children Estante {}
	  }
	   Transform	{
		  translation 343 2 18
		  children Estante {}
	  }

	  Transform	{
		  translation 655 2 18
		  children Estante {}
	  }
	   Transform	{
		  translation 784 2 18
		  children Estante {}
	  }

	  Transform	{
		  translation 967 2 18
		  children Estante {}
	  }
	   Transform	{
		  translation 1096 2 18
		  children Estante {}
	  }


    ]
  }
}

