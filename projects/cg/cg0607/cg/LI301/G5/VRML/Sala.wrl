PROTO Sala
[
]
{
  Group {
    children 
    [
      Transform {
		translation 18 2 0
		children ParedeDaEntrada { }
      }
	  Transform	{
		  translation 0 2 934
		  rotation 0 1 0 1.57
		  children ParedeDoQuadro {}
	  }
	  Transform	{
		  translation 1234 2 934
		  rotation 0 1 0 3.14
		  children ParedeDeFora {}
	  }
	  Transform	{
		  translation 1234 2 934 
		  rotation 0 1 0 1.57
		  children ParedeDoLado {}
	  }
	  Transform	{
		children Chao {}
	  }
	  Transform	{
		  translation 0 -3 0
		  children Tecto {}
	  }

	  Transform	{					  # Distancia do centro é 55 do centro de cada cano

		  translation 376 2 234
		  children Bancos {}
	  }
	  Transform	{
		  translation 510 2 234
		  children Bancos {}
	  }
	  Transform	{
		  translation 638 2 234
		  children Bancos {}
	  }
