PROTO MolduraVidro
[
]
{	
	Group {
		children [

			Transform {
				rotation 1 0 0 -1.57
				translation	 0 0 18
				children
				Shape {
					geometry Extrusion{	
						crossSection [0 0, 264 0,264 62, 0 62, 0 0, 5 5, 259 5,259 57,5 57, 5 5, 0 0 ]            	
						spine [0 0 0, 0 18 0]
						solid FALSE
						}
		 			appearance Appearance {
			    		material  Material	{
							diffuseColor 1 1 1
						}
	    			}

			  }
		  }
		  	Transform {
				translation	 132 31 9
				children
				Shape {
					geometry Box{	
						  size	254 52 12
						}
		 			appearance Appearance {
			    		material  Material	{
							diffuseColor 1 1 1
							transparency 0.5
						}
	    			}
				
			  }
		  }



	  ]
	}
}



PROTO Pilar
[
]
{
	Transform {
		translation	 24 195 24
		children
		Shape {
			geometry Box {
				size 48 390 48
			}
		 	appearance Appearance {
				material Material { diffuseColor  0.6 0.706 0.63 }
				texture	ImageTexture {
			    	repeatS TRUE
					repeatT	TRUE
					url "Texturas\Sala\pilar.JPG" 
				}
				textureTransform  TextureTransform {  
					scale 50.5 40.5 
					rotation 0 
					center 0 0 
					translation 0 0  
				}
	    	}
		}
	}
}

PROTO manta_contra_fogo[]
{
	Group {
		children [
			Transform {
				translation	8 15 2.5
	    		children  Shape	{
					geometry Box { size 16 30 5 }
					appearance Appearance { material Material {	diffuseColor  0.78 0.27 0.06 }}
				}	 
			}
			Transform {
			   translation 8 15 5 
			   children Shape	{
	  				geometry Box { size 16 30 0.1 }
					appearance Appearance   {
						material Material {	diffuseColor  0.78 0.27 0.06 }
						texture	ImageTexture { url "Texturas\Acessorios\manta_contra_fogo.jpg" }   
					}
				}
			}
		]
	}
}      

