PROTO luzCorredor[field SFVec3f pos 0 0 0 , field SFVec3f tamanho 1 1 1, field SFRotation rotacao 0 0 0 0]{

	Transform {
		scale IS tamanho
		rotation IS	rotacao
		translation	IS pos
		children [
			Transform {
				translation	0 -0.005 0 
				children [
					Shape {
						geometry Box{
							size 0.15 0.01 0.15
						}
					  appearance Appearance { 					 						
							material Material {
								diffuseColor .682 .682 .576
								ambientIntensity .945
								shininess .19
								emissiveColor .38 .38 .38
								specularColor .39 .39 .27
							}
						}
					}
				]
			}
			Transform {
				translation	0 0.015 0
				children[ 
					Shape{
						geometry Box{
							size 0.2 0.03 0.2
						}			
						appearance Appearance { 					 						
							material Material {
								diffuseColor 1 .9 .61
								ambientIntensity 0
								shininess 0
								emissiveColor .2 .18 .12
							}
						}
					}					
				]			
			}
		]		
	}
}


