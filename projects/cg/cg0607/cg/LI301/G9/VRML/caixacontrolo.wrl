PROTO texto [field MFFloat len 10, field SFVec3f pos 0 0 0, field SFVec3f sca 1 1 1, field SFColor textColour 1 0 0, field MFString textString "botao" ] 
{
	Transform
	{
		translation	IS pos
		scale IS sca
		children 
		[
			Shape
			{
				geometry Text 
				{
					string    IS textString
					length    IS len
					fontStyle FontStyle 
					{ 
						family       	"VERDANA"
						size         	4.0          
						spacing      	1.0	          
					}
				}
				appearance Appearance 
				{ 
					material Material 
					{
						diffuseColor IS	textColour
						specularColor .19 .03 .03
						ambientIntensity .0833
						shininess .08
					}
				}
			 }
		 ]
	 }
}

PROTO lateral [field SFVec3f pos 0 0 0, field SFVec3f siz 0 0 0, field SFRotation rot 0 0 0 0]
{
	Transform
	{
		rotation IS	rot
		translation	IS pos
		children
		[
			Shape 
			{
				geometry Box
				{
					size IS	siz
				}
				appearance Appearance 
				{ 
					material Material 
					{
						diffuseColor 1 1 0
						specularColor .19 .03 .03
						ambientIntensity .0833
						shininess .08
					}
				}
			}
		]
	}
}

PROTO portaCaixa [field SFVec3f pos 0 0 0]
{


	DEF	portaCaixaAbre 
		Transform
		{
			translation	IS pos
			children
			[
				DEF doorOpens TouchSensor { }

				Transform 
				{
					translation -0.5 0 0
					children
					[
						Shape 
						{
							geometry Box
							{
								size 1 1 0.01
							}
							appearance Appearance 
							{ 
								material Material 
								{
									diffuseColor .8 .51 .09
									specularColor .92 .43 .01
									ambientIntensity .117
									shininess .4
								}
							}
						}
					]
				}

				DEF tempoAbrir TimeSensor 
				{ 
					stopTime 1 
					cycleInterval 2 
				}
			
				DEF	rotOpen OrientationInterpolator
				{
					key[0, 1]
					keyValue [0 1 0 0, 0 1 0 1.57]
				}

			]

			ROUTE doorOpens.touchTime TO tempoAbrir.set_startTime
            ROUTE tempoAbrir.fraction_changed TO rotOpen.set_fraction
			ROUTE rotOpen.value_changed	TO portaCaixaAbre.set_rotation

		}
}

