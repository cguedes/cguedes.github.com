PROTO alavanca [field SFVec3f pos 0 0 0, field SFVec3f sca 1 1 1,field SFRotation rot 0 0 0 0]
{	 
	
	Transform
	{
		children[
			DEF alavancaMovel
			Transform {
				rotation IS	rot
				translation	IS pos
				scale IS sca
				children 
				[
					DEF LeverSwitch TouchSensor { }
					DEF	sensorToque TouchSensor	{ }

		   			DEF matAlav Appearance
					{ 
						material Material 
						{
							diffuseColor .61 .61 .61
							specularColor .77 .76 .76
							ambientIntensity .14
							shininess .9
						}
					}

					Transform
					{
						translation	0.05 0.15 0
						children
						[ 
							DEF	alav Shape
							{
									geometry Cylinder 
									{
    									height 0.15
										radius 0.01
									}
								appearance USE matAlav
							}	 
						]
					}
		
					Transform
					{
						translation	-0.05 0.15 0
						children 
						[
							USE	alav
						]
					}

					Transform
					{
						rotation 0 0 1 1.57
						translation	0 0.225 0
						children 
						[
							USE	alav
						]
					}	

					DEF tempoAbrir TimeSensor 
					{ 
						stopTime 1 
						cycleInterval 2 
					}

					DEF	tempo TimeSensor
					{
					 cycleInterval 2
					 startTime 1
					}
				] 
			}
		]  
	}


	DEF ToogleLever Script 
	{
		field   	SFBool on FALSE
		eventIn 	SFBool set_active
		eventOut	SFRotation on_changed

		url "javascript:
		function set_active( set, timestamp ) 
		{
			if(on == TRUE){
				on = FALSE;
				on_changed[0]=1;
				on_changed[1]=0;
				on_changed[2]=0;
				on_changed[3]=0.7;
			}else{
                on = TRUE;
				on_changed[0]=1;
				on_changed[1]=0;
				on_changed[2]=0;
				on_changed[3]=2;
			}
			return on_changed;
