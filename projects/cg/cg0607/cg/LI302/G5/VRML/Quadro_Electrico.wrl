PROTO porta_quadro[]
{
	DEF	porta_q Transform 
	{
		center -0.18 0 0

		children 
		[
			Shape 
			{
				geometry Box {size 0.35 0.4 0.005}
				appearance Appearance {material Material {diffuseColor 0.85 0.85 0.85}}
			}

			Transform 
		{
			translation	0.1 0 0.004 children
			Shape 
			{
				geometry Box {size 0.03 0.02 0.003}
				appearance Appearance {material Material {diffuseColor 0.85 0.85 0.85}}

			}
		}

		########################################################################
			DEF Touch TouchSensor { }
              DEF OrInterpolator OrientationInterpolator {
                key [0 1] keyValue [0 1 0 0, 0 1 0 -1.57]
              }
              DEF Timer1 TimeSensor {
                stopTime 1
                cycleInterval 2 # Vai demorar este numero de segundos a abrir a porta
                ROUTE Touch.touchTime TO Timer1.startTime
                ROUTE Timer1.fraction_changed TO OrInterpolator.set_fraction
                ROUTE OrInterpolator.value_changed TO porta_q.rotation
              }
			 #########################################################################

		]
	}
}



