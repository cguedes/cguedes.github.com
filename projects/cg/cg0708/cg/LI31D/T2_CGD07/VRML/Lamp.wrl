#VRML V2.0 utf8

PROTO Lamp[]{
	Group {
		children [
			Transform {
				translation 0 16 11.8
				children
				Shape {
					geometry Sphere {radius 1}
					appearance Appearance {
						material Material {
							emissiveColor 1 1 1 
						}
					}
				}
			}

			Shape {
				geometry Box {size 5 0.5 6}
				appearance Appearance {
					material Material {
						diffuseColor 0 0 .72
						specularColor .5 .5 .5
						emissiveColor 0 0 .13
						ambientIntensity 0
					}
				}
			}
			Transform {
				rotation 1 0 0 0.6
				children
				Transform {
					translation	0 9 -2
					children
					Shape {
						geometry Cylinder{height 20	radius 0.5}
						appearance Appearance {
							material Material {
								diffuseColor 0 0 .72
								specularColor .5 .5 .5
								emissiveColor 0 0 .13
								ambientIntensity 0
							} 
						}
					}
				}
			}
			Transform {
				translation	0 16.6 9.86
				rotation 1 0 0 1.57
				children 
				Shape {
					geometry Cylinder {radius 0.5 height 2}
					appearance Appearance {
						material Material {
							diffuseColor 0 0 .72
							specularColor .5 .5 .5
							emissiveColor 0 0 .13
							ambientIntensity 0
						}
					}	
				}
			}
			Transform {
				translation	0 16.5 11.8
				children
				Shape {
					geometry Box {size 3 1 3}
					appearance Appearance {
						material Material{
							diffuseColor 0 0 .72
							specularColor .5 .5 .5
							emissiveColor 0 0 .13
							ambientIntensity 0
						}
					}
				}
			}
		]
	}
}



PROTO LampButton[]{
	Transform {
		translation	0 0.27 1
		children Shape{
			geometry Box {size 1 0.2 1}
			appearance Appearance {
				material Material {
					diffuseColor .75 .75 .75
					specularColor .5 .5 .5
					emissiveColor .15 .15 .15
					ambientIntensity 0
				}
			}
		}
	}

}
