PROTO LogoMetro[field SFRotation rot 0 1 0 1.57, field SFVec3f pos 1035 0 1]{
	Transform {
		rotation IS	rot
		translation	IS pos
		children [
			Shape {
				appearance Appearance {
					textureTransform TextureTransform {
						rotation -1.57
					}
					texture	ImageTexture {
						url	["Imagens\Textura_Logo_Metro.gif"]
					}


				}
				geometry Extrusion {
					crossSection [	8 -1000,
									0 -880.75,
									8 -761.5]
					spine [0 0 0, 0 0 180]
					endCap FALSE
					beginCap FALSE
				}
			}
		]
	}
}


