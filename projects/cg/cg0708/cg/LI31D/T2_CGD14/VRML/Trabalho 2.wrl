		translation	0 219.25 0
		rotation 0 1 0 1.57
		children[
			PortaZonaLivre {}
			Transform {
				translation	0 0 150
				children [
					PortaZonaComJanelaSuperior	{}
					PortaZonaComJanelaInferior {}
					Transform {
						rotation 0 1 0 -1.57
						translation	1.5 -219.25 0
						children Janela {largura [0 0 0, 0 0 550 ]}
					}
				]	
			}
			Transform {
				translation	0 0 700
				children PortaZonaLivre	{}
			}
		]
	}
}

