PROTO Lampada[field SFVec3f position 0 2350 -1350]{
	Group {
		children [
			Transform {
				translation	IS position
				children [

					Shape {
						appearance Appearance {
							material Material {
								diffuseColor 1 1 1
								emissiveColor 1 1 1
							}
						}
						geometry Box {
							size 400 10 100
						}
					}

					PointLight {
						ambientIntensity 0.1       
						attenuation 1 0 0   
						color .9 .9 .9   
						intensity 0.8       
						location 0 0 0
						on TRUE 
						radius 3000
					}
				]
			}
		]
	}
}


