#VRML V2.0 utf8

EXTERNPROTO Coluna [] ["Coluna.wrl#Coluna"]
EXTERNPROTO Sub [] ["Sub.wrl"]

PROTO SetColunas [] {
	Transform {
		children [
		
			Transform {	#Colunas Esq
				translation	-6.15 4.9 -4
				rotation 0 1 0 1.57
				children [ Coluna	{} ]
			}
			Transform {
				translation	-6.15 4.9 0
				rotation 0 1 0 1.57
				children [ Coluna	{} ]
			}
			Transform {
				translation	-6.15 4.9 4
				rotation 0 1 0 1.57
				children [ Coluna	{} ]
			}
			
			Transform {	 #Colunas Dir
				translation	9.68 4.9 -4
				rotation 0 1 0 -1.57
				children [ Coluna	{} ]
			}
			Transform {
				translation	9.68 4.9 0
				rotation 0 1 0 -1.57
				children [ Coluna	{} ]
			}
			Transform {
				translation	9.68 4.9 4
				rotation 0 1 0 -1.57
				children [ Coluna	{} ]
			}

			
			Transform {	# Colunas do fundo
				translation	-4 4.9 -6.17
				#rotation 0 1 0 1.57
				children [ Coluna	{} ]
			}
			Transform {
				translation	4 4.9 -6.17
				#rotation 0 1 0 -1.57
				children [ Coluna	{} ]
			}

			Transform { #Sub
				translation	-1 0.5 10.75
				children [ Sub {} ]
			}
			Transform { #Sub
				translation	1 0.5 10.75
				children [ Sub {} ]
			}
		
		]
	}
}