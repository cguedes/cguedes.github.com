				children PecaCobertura { coverColor IS coverColor }
				scale 0.8 0.18 1.4
			}

			Transform 
			{
				children PecaCobertura { coverColor IS coverColor }
				scale 0.8 0.18 1.6
				translation 4.6 0 -4.00
				rotation	0 1 0 1.57079633
			}

			Transform 
			{
				children PecaCobertura { coverColor IS coverColor }
