#VRML V2.0 utf8


# bancada Inclinada Mais à esquerda
PROTO BancadaComEscadaLeft0 [
	field SFRotation RightOrLeft 0 0 0 0
	field SFVec3f Position 0 0 0
	field MFVec3f Espinha [
		0 0 0, -20 0 0
	]
	field MFVec3f EspinhaEscadas [0 0 0, -2 0 0]
]
{
	Transform {
		translation	IS Position
		rotation IS	RightOrLeft

		children [
			Shape {
				geometry Extrusion { 
					crossSection [
						0 -1, 0 0, 1 -1, 0 -1
					]
					spine IS Espinha #[0 0 0,-1 0 0]
					scale [
						20 25, 10 20
					]
				}
				appearance Appearance {
					material Material { 
					diffuseColor 1 .1 .1 
					shininess .1
					}

					texture	ImageTexture {
						url	"cadeiras.jpg"
					}
				}
			}
			Transform {
				translation	2 0 0
				children [
					Shape {
						geometry Extrusion { 
							crossSection [0 -1, 0 0, 1 -1, 0 -1]
							spine IS EspinhaEscadas
							scale [20 25, 20 25]
							orientation 0 0 1 0
							endCap TRUE
							beginCap TRUE
							solid TRUE
							ccw TRUE
							convex TRUE
							creaseAngle 0
						}
						appearance Appearance {
							material Material { 
							ambientIntensity 0.2
							diffuseColor 0.5 0.5 0.5 
							emissiveColor 0 0 0
							shininess 0.1
							specularColor 0 0 0 
							transparency 0
							}
						}
					}
				]
			}


		]
	}
}



