PROTO BancadaPiso2 [ ]{
	Group {
		children [
			BancadaCentral { 
				DuplicatePos 0 0 -54
			}

			BancadaCentral {
				Duplicate 0 1 0 3.14
				DuplicatePos -1 0 54
			}
		]
	}
}
