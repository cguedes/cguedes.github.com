PROTO _axis [ field SFColor _color 0 1 0, field MFString _name "Y" ]{
	
	Transform {
		children [
			Transform {
				translation	0 12.5 0
				children [
					Shape {
						geometry Cylinder {
							radius .1
							height 25
							top	TRUE
						}
						appearance Appearance {
							material Material {
								diffuseColor IS _color
								transparency .7
							}
						}
					}

					Transform {
						translation	0 10 0
						children [
							Shape {
								geometry Text {
									string IS _name
									length 1
									fontStyle FontStyle	{
										style "BOLD"
										family "ARIAL"
										size 2
									}
								}
								appearance Appearance {
									material Material {
										diffuseColor 1 1 1
									}
								}
							}
						]
					}

				]
			}

			Transform {
				translation	0 25 0
				children [
					Shape {
						geometry Cone {
						   bottomRadius	.3
						   height 1.5
						   bottom FALSE
						}
						appearance Appearance {
							material Material {
								diffuseColor IS _color
								transparency .7
							}
						}
					}
				]
			}
		]
	}
}


PROTO _cube[field SFVec3f _size 2 .2 2 exposedField MFString _texture "", 
			field SFFloat _transparencia 0,
			field SFColor _color .8 .8 .8 ]{
	
	Shape {
		geometry Box {
			size IS	_size
		}
		appearance Appearance {
			material Material {
				diffuseColor IS	_color
				transparency IS	_transparencia
			}
			texture	ImageTexture {
				url	IS _texture
			}
		}
	}
}







