PROTO _sala_de_aula[ field SFRotation _rotacao_porta 0 1 0 3.1417, 
					 field SFVec3f _translaccao_porta -45 -19 -.3, 
					 field SFVec3f _translacao_rodape 0 -17.5 0,
					 field SFRotation _porta_moldura 0 0 1 0,
					 field MFVec2f _crossSectionParede 
							[ 0 0, 45 0, 45 1, 1 1, 1 79, 64 79, 
								64 1, 60 1, 60 0, 65 0, 65 80, 0 80, 0 0 ],
					 field MFVec2f _crossSectionRodape 
							[ -.2 -.2, 45.2 -.2, 45.2 1.2, 1.2 1.2, 1.2 78.8, 63.8 78.8, 
						           63.8 1.2, 60 1.2, 60 -.2, 65.2 -.2, 65.2 80.2, -.2 80.2, -.2 -.2 ],
				   ]{

	# Sala com a porta no lado esquerdo #
	# _translaccao_porta  -60 0 1.3
	# _rotacao_porta 0 0 1 3.1417
	# _translaccao_rodape 0 0 0

	# Sala com a porta do lado direito  #
	# _translaccao_rodape 0 -17.5 0
	# _translaccao_porta -75 -19 1.3
	# _rotacao_porta 0 0 1 0


	# Configuraçao para sala quadrada..
	# crossSection [ 0 0, 45 0, 45 1, 1 1, 1 64, 64 64, 
	#               64 1, 60 1, 60 0, 65 0, 65 65, 0 65, 0 0 ]

	# Configuraçao para sala Rectangular
	# crossSection [ 0 0, 45 0, 45 1, 1 1, 1 79, 64 79, 
	#               64 1, 60 1, 60 0, 65 0, 65 80, 0 80, 0 0 ]

	# Rodapé para sala quadrada..
	# crossSection [ -.2 -.2, 45.2 -.2, 45.2 1.2, 1.2 1.2, 1.2 63.8, 63.8 63.8, 
	#                63.8 1.2, 60 1.2, 60 -.2, 65.2 -.2, 65.2 65.2, -.2 65.2, -.2 -.2 ]

	# Rodapé para sala rectagular..
	# crossSection [ -.2 -.2, 45.2 -.2, 45.2 1.2, 1.2 1.2, 1.2 78.8, 63.8 78.8, 
	#                63.8 1.2, 60 1.2, 60 -.2, 65.2 -.2, 65.2 80.2, -.2 80.2, -.2 -.2 ]

	Transform {
		children [
			DEF _forma_sala Shape {
				geometry Extrusion {


					crossSection IS	_crossSectionParede
					solid FALSE
					spine [0 0 0, 0 -19 0]
				}
				appearance Appearance {
					material Material {}
				}
			}

			# Rodapé... #
			Transform {
				translation	IS _translacao_rodape
				children [ 
					Shape {
						geometry Extrusion {
							crossSection IS	_crossSectionRodape
							solid FALSE
							spine [0 0 0, 0 -1.5 0]
						}
						appearance Appearance {
							material Material { diffuseColor 1 0.5 0 }
							texture	ImageTexture { url "texture\porta_madeira.jpg"	}
						}
						
					}
			    ]
			}

			Transform {
				translation	IS _translaccao_porta #-75 -19 1.3
				rotation IS	_rotacao_porta
				children [ _porta_sala_de_aula { _rotacao_porta IS _rotacao_porta } ]
			}

		]
	}
}




