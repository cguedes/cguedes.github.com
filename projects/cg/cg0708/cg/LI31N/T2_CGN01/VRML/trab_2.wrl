PROTO _fech_base[ field SFVec3f _translation 0 0 0 ]{

	Transform {
		translation	.2 -6.5 .5
		children [
			DEF _suporte Shape	{
				geometry Extrusion {
					crossSection [ 0 0, 1.5 0, 1.5 .2, .85 .6, .45 .6, 0 .35, 0 0 ]
					spine [0 0 0, .25 0 0]
					solid FALSE
				}
				appearance Appearance {
					material Material { 
						diffuseColor 0 0 0 
						specularColor .29 .3 .29
						ambientIntensity 0
						shininess .08
					}
					texture	ImageTexture {

					}
				}
			}


			Transform {
				translation	-7.6 0 0
				children [ USE _suporte ]
			}

			Transform {
				translation	IS _translation	# -7.6 0 0 muda a vara para o lado esquero
				children [
					Transform {
						translation	.12 -1 .1
						children [
						   Shape {
								geometry Cylinder {
								   radius .045
								   height 14
								}
								appearance Appearance {
									material Material { diffuseColor 0 0 0 
									specularColor .63 .92 1
									ambientIntensity 0
								}
								}
							}
						]
					}

					DEF _bases Shape{
						geometry Extrusion {
							crossSection [ 0 0, .7 0, .7 .15, .4 .3, .09 .3, 0 0  ]
							spine [0 0 0, .25 0 0]
							ccw	FALSE
						}
						appearance Appearance {
							material Material {
								diffuseColor 0 0 0
								specularColor .29 .3 .29
								ambientIntensity 0
								shininess .08
							}
						}
					}

					Transform {
						translation	0 6.3 0
						children [ USE _bases ]
					}

					Transform {
						translation	.25 -8.4 0
						rotation 0 0 1 3.1416
						children [ USE _bases ]
					}

			

					Transform {
						translation	.02 3 0
						children [ 
							DEF	_no Shape {
								geometry Extrusion {
									crossSection [ 0 0, .2 0, .2 .13, .16 .17, .04 .17, 0 .13, 0 0 ]
									spine [0 0 0, 0 .08 0]
									solid FALSE
								}
								appearance Appearance {
									material Material {
										diffuseColor 0 0 0
										specularColor .29 .3 .29
										ambientIntensity 0
										shininess .08
									}
								}
							}

							Transform {
								translation	0 -7.5 0
								children [ USE _no ]
							}	
						]
					}
				]
			}
		]
	}
}


