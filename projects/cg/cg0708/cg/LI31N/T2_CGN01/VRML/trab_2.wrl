PROTO _puxador_porta[]{

		PROTO _cyl[ field SFFloat _altura .35, field SFFloat _raio .04 ]{
			Shape {
				geometry Cylinder {
					height IS _altura
					radius IS _raio
				}
				appearance Appearance {
					material Material { 
						diffuseColor .37 .37 .37
						specularColor .89 .89 .89
						ambientIntensity .1
						shininess .13
					}
				}
			}	
		}

		Transform {
			children [
	
				# Base cilindrica..  parte mais pequena do 'L' #
				Transform {
					rotation 1 0 0 1.57
					children [ _cyl	{} ]
				}

				# Parte Horizontal.. parte maior do 'L' #
				Transform {
					translation	.37 0 .2
					rotation 0 0 1 1.57
					children [ _cyl { _altura .8, _raio .042 } ]
				}

				# Base do puxador.. #
				Transform {
					translation	0 0 0
					rotation 0 1 0 1.57
					children [ 

						Transform {
							rotation 0 0 1 1.57
							children [ _cyl{ _altura .02, _raio .2} ]
						}

						Transform {
							translation	0 -.7 0
							rotation 0 0 1 1.57
							children [
								_cyl { _altura .04, _raio .1 }
								_cyl { _altura .02, _raio .2 }
							]
						}
			
					]
				}

			]
		}
}



