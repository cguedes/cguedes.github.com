PROTO _porta_entrada[  field SFVec3f _posicao_vara 0 0 0, field MFString _texture "texture\parede.jpg" field MFVec3f _spine [0 0 0, 0 0 .4] ]{
	# mudar a posiçao da vara.. _posicao_vara -7.6 0 0		
	Transform {
		rotation 0 1 0 3.1416
		children [

			Shape {
				geometry Extrusion {
					crossSection [ 0 0, 8.4 0, 8.4 15, 0 15, 0 0, .8 .8, 7.6 .8, 7.6 14.2, .8 14.2, .8 .8 ]
