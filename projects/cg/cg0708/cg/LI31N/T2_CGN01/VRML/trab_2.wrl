PROTO _grelha[]{

	Group {
		children [

			Transform {
				translation	1.55 -.1 8.7
				children [
					Shape {
						geometry Extrusion {
							crossSection [ 0 0, 0 4, 4 4, 4 0, 0 0, .3 .3, .3 3.7, 3.7 3.7, 3.7 .3, .3 .3, 0 0 ]
							spine [0 0 0, 0 .15 0]
							solid FALSE
						}
						appearance Appearance {
							material Material { diffuseColor 1 1 1 }
						}
					}
				]
			}

			Transform {
				translation	3.55 -.1 8.8
				rotation 1 0 0 1.57
				scale .9 1.16 1
				children [ 
					_estores { _ripasColor 1 1 1} 
					Transform { 
						translation	0 1.03 0
						children [ _estores {_ripasColor 1 1 1} ] 
					}

					Transform { 
						translation	0 2.06 0
						children [ _estores {_ripasColor 1 1 1} ] 
					}
				]
			}
		]
	}

}


PROTO _porta_normal[ ]{
	Transform {
		children [
			Shape	{
				geometry Extrusion {
					crossSection [ 0 0, 7.4 0, 7.4 15, 0 15, 0 0, .8 .8, 6.6 .8, 6.6 14.2, .8 14.2, .8 .8 ]
					solid FALSE
					spine [0 0 0, 0 .4 0]
				}
				appearance Appearance {
					material Material {}
					texture	ImageTexture { url	"texture\porta_madeira.jpg" }
				}
			}

			Transform {
				translation	0 .4 .5
				children [

					_grelha	{}

					Transform {
						translation	3.7 -.2 7
						children [ _cube{ _size 6 .39 14, _texture "texture\porta_madeira.jpg" } ]
					}


					Transform {
						translation	.4 0 6
						rotation 1 0 0 -1.57
						children [ _puxador_porta { } ] 
					}

					
				]
			}

			Transform {
				translation	0 .5 15.05
				rotation 1 0 0 3.1417
				children [
					Shape {
						geometry Extrusion {
							crossSection [ -1 0, -1 19, 8.4 19, 8.4 0, 7.5 0, 7.5 15.1, -.1 15.1, 
											-.1 16, -.1 18.1, 7.5 18.1, 7.5 16, -.1 16, -.1 0, -.1 0 ]
							spine [0 0 0, 0 1 0]
							solid FALSE

						}
						appearance Appearance {
							material Material {}
							texture	ImageTexture { url "texture\porta_madeira.jpg"}
						}
					}

					Transform {
						translation	3.5 .5 17
						children [ _cube { _size 7.7 .3 2.3, _transparencia .5 } ]
					}
				]
			}

		]
	}
}



PROTO _window[]{
	Transform {
		children [

			DEF _line Transform {
				children [ 
					_moldura{}
					Transform {
						translation	16 0 0
						children [ _moldura {} ]
					}
					Transform {
						translation	8.4 2.5 .25
						children [ _cube { _size .8 4 .5 } ]
					}
					Transform {
						translation	25.2 2.5 .25
						children [ _cube { _size .8 4 .5 } ]
					}

					Transform {
						translation	16.4 1.3 .8
						scale 8 3.3 1.4
						children [ _estores { _transparencia .5, _ripasColor 0.55 0.6 0.55 } ]
					}

					DEF _vline Transform {
						translation	8.4 2.7 .8
						children [ _cube { _size .05 4.5 .1, _color 0 0 0 } ]
					}

					Transform {
						translation	8.4 0 0
						children [ USE _vline ]
					}

					Transform {
						translation	16.8 0 0
						children [ USE _vline ]
					}

    			]
			}

			Transform {
				translation	0 4 0
				children [ USE _line ]
			}

			Transform {
				translation	0 8 0
				children [ USE _line ]
			}

		]
	}
}


PROTO _janela_grande[ field SFColor _cor_da_parede .9 .9 .9 ]{
	Transform {
		children [
			Shape	{
				geometry Extrusion {
					crossSection [ 0 0, 0 22, 3 22, 3 8.7, 36.05 8.7, 36.05 22, 39.05 22, 39.2 8.7,
									72.2 8.7, 72.2 22, 75.5 22, 75.2 0, 0 0 ]
									spine [0 0 0, 0 0 -1.5]
					solid FALSE
				}
				appearance Appearance {
					material Material { diffuseColor IS _cor_da_parede }
				}
			}

			DEF _w Transform {
				translation	3.05 8.8 -1.1
				children [ _window {} ]
			}

			Transform {
				translation	36.5 0 0
				children [ USE  _w ]
			}

		]
	}
}







