					solid FALSE
				}
				appearance Appearance {
					material Material { }
					texture	ImageTexture {
						url	IS _texture
					}
				}
			}

			Transform{ 
				translation	4.2 -7.5 .3
				children [ _cube { _size 8 13.5 .3, _transparencia .6 } ] 
			}

