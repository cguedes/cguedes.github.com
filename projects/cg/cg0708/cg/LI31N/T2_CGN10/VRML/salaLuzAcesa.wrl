PROTO ESCADA [field	SFColor	escadaColor 0.5 0.5 0.5]{
	Transform {
		children [
			Transform {
				translation	0 0 0
				children [
					Shape {
						geometry Box {size 30 0.25 20}
						appearance Appearance {
							material Material {
								diffuseColor IS	escadaColor	
							}
							texture	ImageTexture { url "fotos/chao.png" repeatS TRUE repeatT TRUE }
						}
					} 
				]

			}
		]
	}
}

PROTO LUZ [field SFColor LuzColor 1 1 1]{
	Transform {
		children[

			Transform {
				translation	0 0 0
				children [
					Shape {
						geometry Sphere	{radius 0.2}
						appearance Appearance {
							material Material {
								diffuseColor 1 1 1	
							}
						} 	

					}
				]
			}
			
			SpotLight{
				location 0.0 0.0 0.0 # posição da luz
				direction 0.0 -1.0 0.0 # direcção da luz
				cutOffAngle 0.785 # abertura máxima do cone de luz
				beamWidth 0.52# cone com brilho constante
				intensity 1.0
				color 1.0 1.0 0.75 
				on TRUE
			}
				
		]
	}
}


PROTO ColunaE [] {
