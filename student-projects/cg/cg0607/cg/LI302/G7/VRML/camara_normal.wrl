########################################################################
# PROTO de Parafuso
########################################################################

PROTO ScrewTop[]{
	Transform {
		children [
			Shape {
				geometry Extrusion {
					spine [
						.4 .2 0, .3 .0267949 0, .1 .0267949 0, 0 .2 0, .1 .373205 0,
						.3 .373205 0, .4 .2 0
					]
					crossSection [
						.2 .2, .2 -.2, -.2 -.2, -.2 .2, .2 .2
					]
				}
			
				appearance Appearance {
					material Material {
						diffuseColor .47 .52 .58
						specularColor 1 1 1
						ambientIntensity .123
						shininess .13
					}
				}
			}
		]
	}
}
PROTO ScrewEnd []{
	Transform {
		children [
			Shape {
				geometry Cylinder {
					height 5
					radius .2
				}
				
				appearance Appearance {
					material Material {
						diffuseColor .47 .52 .58
						specularColor 1 1 1
						ambientIntensity .123
						shininess .13
					}
				}
			}
		]
	}
}

PROTO Porca []{
	Transform {
		children [
			Shape { # Shape 1
				appearance Appearance {
					material Material {
						diffuseColor .47 .52 .58
						specularColor 1 1 1
						ambientIntensity .123
						shininess .13
					}
				}
				geometry Extrusion {
					spine [
						.6 0 0, .5 -.3 0, .3 -.5 0, 0 -.6 0, -.4 -.5 0, -.6 -.3 0,
						-.7 0 0, -.6 .3 0, -.375 .519615 0, 0 .6 0, .275 .519615 0,
						.5 .3 0, .6 0 0
					]
					scale [
						1.3 1.3, 1 1, 1.5 1.5, 1 1, 1.5 1.5, .9 .9, 1.3 1.3, .9 .9,
						1.4 1.4, 1 1, 1.5 1.5, 1 1, 1.3 1.3
					]
					crossSection [
						.3 .6, .3 0, 0 0, 0 .6, .3 .6
					]
				}
			}
		]
	}
}

PROTO Screw []{
	Transform {
		children [
			Transform {
				children ScrewTop {}
			}

			Transform {
				rotation 1 0 0 1.57
				translation	.2 .2 2.6
				children ScrewEnd {}
			}

			Transform {
				scale .31 .31 .31
				rotation 0 0 0 1.57
				translation	.22 .2 4.062
				children Porca {}
			}
		]
	}
}

########################################################################
# Proto de Braçadeira
########################################################################

PROTO Bracelet[]{
	Transform {
		children [
			Shape { # Shape 1
				appearance Appearance {
					material Material {
						diffuseColor .47 .52 .58
						specularColor 1 1 1
						ambientIntensity .123
						shininess .13
					}
				}
				geometry Extrusion {
					crossSection [
						-1.9 0, -1.9 .2, -.7 .2, -.6 .3, -.4 .4, -.2 .5, 0 .5, .2 .5,
						.4 .4, .6 .3, .7 .2, 1.9 .2, 1.9 0, .6 0, .5 .1, .4 .2, .2 .3,
						.1 .3, -.1 .3, -.2 .3, -.4 .2, -.5 .1, -.6 0, -1.9 0
					]
					spine [
						0 0 0, 0 .6 0
					]
				}
			}
		]
	}
}

