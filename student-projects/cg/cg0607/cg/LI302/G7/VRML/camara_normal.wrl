PROTO CamArm [] {
	Transform {
		children
		[
			Transform {
				children
				Shape {
					geometry Box { size .08 .08 .008 }
		
					appearance Appearance {
						material Material {
							diffuseColor .945 .937 .965
							ambientIntensity 0
							shininess 0
						}
					}
				}
			}
			#Parafuso 1
			Transform {
				scale .01 .01 .01
				translation	.025 .025 -.006
				children Screw {}
			}
			#Parafuso 2
			Transform {				
				children[ 
					Transform {
						scale .01 .01 .01
						translation	-.025 .025 -.006
