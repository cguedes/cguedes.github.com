DEF	myArmDownInterpolator PositionInterpolator {
	key		 [0,		 0.25,		0.5,	   0.75,	  1]
	keyValue [0 0 0, 0 -0.025 0, 0 -0.05 0, 0 -0.075 0, 0 -0.1 0]
}
DEF	myArmUpInterpolator PositionInterpolator {
	key		 [0,		0.25,		0.5,	   0.75,	  1]
	keyValue [0 -0.1 0, 0 -0.075 0, 0 -0.05 0, 0 -0.025 0, 0 0 0]
}

DEF	time1 TimeSensor	{
	cycleInterval 3 loop FALSE stopTime	1
}
DEF	time2 TimeSensor	{
	cycleInterval 3 loop FALSE stopTime	1
}

ROUTE myTouch_up.touchTime TO time1.startTime
ROUTE myTouch_down.touchTime TO time2.startTime
ROUTE time1.fraction_changed TO myArmUpInterpolator.set_fraction
ROUTE time2.fraction_changed TO myArmDownInterpolator.set_fraction
ROUTE myArmUpInterpolator.value_changed TO moveable_top.translation
ROUTE myArmDownInterpolator.value_changed TO moveable_top.translation

DEF	EnableSwitch Script	{
	eventIn	SFTime touch
	eventOut SFBool	enable1
	eventOut SFBool	enable2
	field SFBool inicial TRUE

	url	"javascript:
	function touch(value) {
		if(inicial == TRUE){
			enable1 = FALSE;
			enable2 = TRUE;
			inicial = FALSE;
		}
		else{
			enable1 = TRUE;
			enable2 = FALSE;
			inicial = TRUE	;
		}
	}
	"
}

ROUTE myTouch_up.touchTime  TO EnableSwitch.touch
ROUTE myTouch_down.touchTime TO EnableSwitch.touch
ROUTE EnableSwitch.enable1    TO myTouch_down.enabled
ROUTE EnableSwitch.enable2    TO myTouch_up.enabled