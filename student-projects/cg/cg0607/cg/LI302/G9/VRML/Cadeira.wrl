PROTO Perna[]{
	Shape{
		geometry Extrusion {
			crossSection [0.01 0, 0.05 0, 0.055 0.01, 0.055 0.02, 0.05 0.03, 0.01 0.03, 0.005 0.02, 0.005 0.01, 0.01 0]
			spine [-0.34 -0.03 0, -0.02 -0.03 0, -0.02 -0.025 0, -0.015 -0.02 0, 0 0 0, -0.06 0.33 0, 0 0.64 0]
			beginCap TRUE
			endCap TRUE
			ccw	TRUE
			solid FALSE
			convex TRUE
		}
		appearance Appearance {
			material Material {
				diffuseColor .61 .61 .61
				specularColor .39 .39 .39
				ambientIntensity .25
				shininess 1
			}
		}
	}
}

PROTO FerroCentral[field MFVec3f Tamanho[]]{
	Shape {
		geometry Extrusion {
		   crossSection [0.01 0, 0.05 0, 0.055 0.01, 0.055 0.02, 0.05 0.03, 0.01 0.03, 0.005 0.02, 0.005 0.01, 0.01 0]
		   spine IS Tamanho
		   beginCap TRUE
		   endCap TRUE
		   ccw	TRUE
		   solid FALSE
		   convex TRUE
		}
		appearance Appearance {
			material Material {
				diffuseColor .61 .61 .61
				specularColor .39 .39 .39
				ambientIntensity .25
				shininess 1
			}
		}
	}
}

PROTO SuporteTampo[]{
	Shape {
		geometry Extrusion {
		   crossSection [0 0.01, 0 0.05, 0.01 0.055, 0.02 0.055, 0.03 0.05, 0.03 0.01, 0.02 0.005, 0.01 0.005, 0 0.01]
		   spine [0 0 -0.157, 0 0 0.15, 0.01 0 0.165, 0.02 0 0.18, 0.03 0 0.195, 0.035 0 0.2, 0.16 0 0.20]
		   beginCap TRUE
		   endCap TRUE
		   ccw	TRUE
		   solid FALSE
		   convex TRUE
		}
		appearance Appearance {
			material Material {
				diffuseColor .61 .61 .61
				specularColor .39 .39 .39
				ambientIntensity .25
				shininess 1
			}
		}
	}
}
PROTO Ponta[]{
	Shape {
		geometry Extrusion {
		   crossSection [0 0.01, 0 0.05, 0.01 0.055, 0.02 0.055, 0.03 0.05, 0.03 0.01, 0.02 0.005, 0.01 0.005, 0 0.01]
		   spine[0 0 0, 0 0 0.01]
		   beginCap TRUE
		   endCap TRUE
		   ccw	TRUE
		   solid FALSE
		   convex TRUE
		}
		appearance Appearance {
			material Material {
				diffuseColor 0 0 0
			}
		}
		
	}
}
