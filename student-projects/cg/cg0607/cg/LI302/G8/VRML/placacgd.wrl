#VRML V2.0 utf8

PROTO placaCGD [
	exposedField SFRotation rotation 0 0 0 0
	exposedField SFVec3f translation 0 0 0
]{
	Transform {
		rotation IS	rotation
		translation	IS translation
		children [
			Shape {
				geometry Box {
					size 0.40 0.2 0
				}

				appearance Appearance {
					texture	ImageTexture {
    					url [ "placapeq.png" ] 
    					repeatS FALSE
    					repeatT FALSE
					}
				}
			}
		]
	}
}
