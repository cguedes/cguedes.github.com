#VRML V2.0 utf8

PROTO altura_arco

[
] 
{
	Shape {
		appearance Appearance {material Material { diffuseColor 0.8 0.8 0.8 } }
		geometry Cylinder {
			bottom TRUE
			height 0.02
			radius 0.001
			side TRUE
			top	TRUE
		}
	}

	
}

PROTO largura_arco

[
] 
{
	Shape {
		appearance Appearance {material Material { diffuseColor 0.8 0.8 0.8 } }
		geometry Cylinder {
			bottom TRUE
			height 0.002
			radius 0.0001
			side TRUE
			top	TRUE
		}
	}

	
}

PROTO esfera
[
]
{
	
	Shape {
		appearance Appearance {material Material { diffuseColor 0.8 0.8 0.8 } }
		geometry Sphere	{

			radius 0.001
		}
	
	}
}


PROTO argola
[
]
{
	Group {
		children [
		
			Transform{
				children altura_arco {}
			}
			Transform{
				translation	0.01 0.01 0
				rotation 0 0 1 1.57
				children altura_arco {}
			}
		
		   Transform{
				translation	0.01 -0.01 0
				rotation 0 0 1 1.57
				children altura_arco {}
			}

		   Transform{
				translation	0.02 0 0
				children altura_arco {}
			}

			Transform {
					translation	0.0 0.01 0
					children esfera	{ }
			}
			Transform {
					translation	0.02 0.01 0
					children esfera	{ }
			}
			Transform {
					translation	0.0 -0.01 0
					children esfera	{ }
			}
			Transform {
					translation	0.02 -0.01 0
					children esfera	{ }
			}		
		]
	}
}

