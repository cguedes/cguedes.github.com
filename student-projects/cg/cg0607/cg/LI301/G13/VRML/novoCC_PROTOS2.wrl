PROTO tecto
[
       eventIn SFTime startAnimation
       eventIn SFTime disappear
]
{
       Group {
               children [
                       DEF chao1 floor {sizeFloor 63.60 0.1 18.25, translationFloor 31.8 0.05 9.125}
                       DEF chao2 floor {sizeFloor 3 0.1 7.85, translationFloor 65.1 0.05 3.925}
               ]
       }
       DEF     Andar1Scripts Script {
               eventIn SFTime startAnimation IS startAnimation
               eventIn SFTime disappear IS disappear
               eventOut SFTime startAnimationOut
               eventOut SFTime disappearOut

               url     "javascript:
                       function startAnimation(value) {
                               startAnimationOut = value;
                       }
                       function disappear(value) {
                               disappearOut = value;
                       }
               "
       }
       ROUTE Andar1Scripts.startAnimationOut TO chao1.appear
       ROUTE Andar1Scripts.disappearOut TO chao1.disappear
       ROUTE Andar1Scripts.startAnimationOut TO chao2.appear
       ROUTE Andar1Scripts.disappearOut TO chao2.disappear

}