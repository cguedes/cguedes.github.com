PROTO	Ventoinha [
]{
	Shape	{
		geometry Box { size	0.12 0.12 0.001 }
		appearance Appearance	{
			texture	ImageTexture { url "imagens/ventoinha.jpg" }
		}
	}
}

PROTO	Ventilador [
]{
	Group	{
		children [
			Transform	{
				children 
					Shape {
						geometry Box {size 0.52 0.55 0.10	}
						appearance Appearance	{	material Material	{ diffuseColor 0.9 0.9 0.9}}
					}
			}
			Transform{
				translation -0.16 0.17 0.0505
				children Ventoinha {} 				
			}
			Transform{
				translation 0.16 0.17 0.0505
				children Ventoinha {}				
			}
		]
	}
}

