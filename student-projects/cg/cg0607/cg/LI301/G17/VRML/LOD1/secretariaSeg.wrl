PROTO SecretariaSeg [] {
	Transform {
		translation	0 0.375 0
		children [
			Group {
				children [
					Transform {
						translation	0.6 0 0.40
						children Mesa3 { }
					}
					Transform {
						translation	0.6 0.375 0.425
						children Mesa3_Top { }
					}
					#a mesa que está mais separada + 0.5m
					Transform {
						translation	2.3 0 0.40
						children Mesa3 { }
					}
					Transform {
						translation	2.3 0.375 0.425
						children Mesa3_Top { }
					}
					Transform {
						rotation 0 1 0 -1.57
						children Mesa2 { }
					}
					Transform {
						translation	0 0.375 0
						rotation 0 1 0 -1.57
						children Mesa2_Top { }
					}
					Transform {
						translation	-0.4 0 -0.4
						children Mesa1 { }
					}
					Transform {
						translation	-0.425 0.375 -0.4
						children Mesa1_Top { }
					}
				]
			}
		]
	}
}

