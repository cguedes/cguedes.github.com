##########################################################################################################
# Puxador Portas de Vidro

PROTO SeccaoCurvaPuxPVidro[

	field SFColor corSeccao 0 0 0
	
]{
	# Em centimetros
	Shape {
		appearance Appearance	{
			material Material {
       			diffuseColor IS	corSeccao
				ambientIntensity 0.5
				shininess 0.8
   			}
		}
		geometry Extrusion {
			crossSection [ 
				# Circulo raio 1
				1.00  0.00,   0.92 -0.38,
				0.71 -0.71,   0.38 -0.92,
				0.00 -1.00,  -0.38 -0.92,
				-0.71 -0.71,  -0.92 -0.38,
				-1.00 -0.00,  -0.92  0.38,
				-0.71  0.71,  -0.38  0.92,
				0.00  1.00,   0.38  0.92,
				0.71  0.71,   0.92  0.38,
				1.00  0.00
			]
			# Passar a raio 2 
			scale 2 2
			# curvatura
			spine [ 0 0 0, 0 2 0, -0.2 3 0, -0.7 4 0, -2 4.7 0,-3 5 0, -5 5 0]
			creaseAngle 0.7
		}
	}
}

PROTO PuxadorPortaVidro[
]{
	Transform {
		# Escala para metros
		scale 0.01 0.01 0.01
		# Colocar vertical
		rotation 1 0 0 1.5708

		children [
			# 1ª Secção
			SeccaoCurvaPuxPVidro{}
			# 2ª Secção
			Transform {
				translation	-5 5 0
				rotation 0 0 1 1.5708
				children [
					Transform {
						rotation 0 1 0 1.5708
						children
							SeccaoCurvaPuxPVidro{}
					}
				]
			}
			# Cilindro central
			Transform {
				rotation 1 0 0 1.5708
				translation	-10 5 10
				children [
					Shape {
						appearance Appearance {
							material Material {
								diffuseColor 1 1 1
								ambientIntensity 0.4
								shininess 0.3
								specularColor 0.8 0.8 0.8
							}
						}
						geometry Cylinder {
							height 10
							radius 2
							side TRUE
						}
					}
				]
			}
			# 3ª Secção
			Transform {
				translation	-5 5 20
				rotation 0 1 0 -1.5708
				children [
					Transform {
						rotation 1 0 0 1.5708
						children
							SeccaoCurvaPuxPVidro{}
					}
				]
			}
			# 4ª Secção
			Transform {
				translation	0 0 20
				children [
					SeccaoCurvaPuxPVidro{}
				]
			} 
		]
	}
}

