							crossSection [0 0,0.1 0,0.1 2.25, 1.8 2.25,
										  1.8 0,1.9 0,1.9 2.3,0 2.3,0 0]
							spine [0 0 0,0 0.09 0]
							solid FALSE

						}
					}
				]
			}
		]
	}
}

