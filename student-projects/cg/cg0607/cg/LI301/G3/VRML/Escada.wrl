PROTO pilarCorrimao1 
[
	field SFVec3f trpilarCorrimao1 0 0 0
]{
	Transform {
		translation	IS trpilarCorrimao1
		children
		Transform {
			translation	0.02 0.518 0.02
			children
			Shape {
				geometry Box {
					size 0.04 1.05 0.04
				}
				appearance Appearance {
					material Material {
						diffuseColor 0.8 0.8 0.8
					}
				}
			}
		}
	}
}

