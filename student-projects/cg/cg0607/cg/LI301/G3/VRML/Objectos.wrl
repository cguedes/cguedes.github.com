# Ecrã Plasma

PROTO Plasma
[
	field SFVec3f trPlasma 0 0 0 
	field SFRotation rotPlasma 0 0 0 0
]
{

	Transform{
		translation	IS trPlasma
		rotation IS	rotPlasma
		children[
			Transform{
				rotation 1 0 0 1.571
				children 
				Shape  {
					appearance Appearance {
						material Material {
							diffuseColor 0 0 0
						}
					}
					geometry Extrusion {
						crossSection [-0.75 -0.4, 0.75 -0.4, 0.75 0.4, -0.75 0.4, -0.75 -0.4]
						spine [0 0 0, 0 0.08 0]
						scale [ 1 1, 0.7 0.7]
						solid FALSE
					}
				}	
			}

			Group{
				children 
				Shape  {
					appearance Appearance {
						material Material {
							diffuseColor 0 0 0
						}
					}
					geometry Box {
								size 1.5 0.8 0.02
					}
				}	
			}


			Transform{
				translation 0 0.02 -0.008
				children 
					Shape {
					 appearance Appearance {
    					texture ImageTexture {
    					  url ["imagem_LCD.jpg"]
		  
    					} 
					}
					geometry Box {
								size 1.35 0.65 0.005
					}
				}	
			}

			Transform{
				translation 0 -0.36 -0.008
				children 
				Shape {
					appearance Appearance {
    					texture ImageTexture {
    						url ["panasonic.jpg"]
    					} 
					}
					geometry Box {
						size 0.15 0.08 0.005
					}
				}	
			}	

			Transform{
				rotation 1 0 0 1.571
				translation 0.5 -0.36 -0.006
				children 
					Shape {
					appearance Appearance {
						material Material {
							diffuseColor 0 1 0
						} 
					}
					geometry Cylinder {
						bottom TRUE
						height 0.01
						radius 0.008
						side TRUE
						top	TRUE
					}
				}	
			}	
		]
	}   
}

