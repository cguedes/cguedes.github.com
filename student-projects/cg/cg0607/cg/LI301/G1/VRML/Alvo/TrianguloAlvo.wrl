#VRML V2.0 utf8
EXTERNPROTO	eixo[]"eixo.wrl#teste"
PROTO Triang[
	field MFVec2f crossSection [
		0			0, 
		1			0,
		0.999876758	0.015699355,
		0.999507061	0.03139484,
		0.998891	0.047082587,
		0.998028728	0.062758729,
		0.996920457	0.078419402,
		0.99556646	0.094060746,
		0.993967071	0.109678905,
		0.992122684	0.12527003 ,
		0.990033753	0.140830277,
		0.987700795	0.156355812,
		0.985124383	0.171842808,
		0.982305152	0.187287447,
		0.979243798	0.202685923,
		0.975941076	0.218034439,
		0.972397798	0.233329214,
		0.968614839	0.248566476,
		0.964593132	0.26374247 ,
		0.960333666	0.278853456,
		0.955837493	0.293895708,
		0.95110572	0.30886552 ,
		0			0
								]
	field MFVec3f spine [0 0 0, 0 0 0.01]
	field SFColor color 1 1 1
]{
	Shape {
		geometry Extrusion {
			beginCap TRUE
			endCap TRUE
			ccw	TRUE 
			convex	TRUE
			creaseAngle	0
			crossSection IS	crossSection
			spine IS spine
			solid TRUE
			scale	1 1
		}
		appearance Appearance {material Material {diffuseColor IS color}}
	}
	
}
PROTO trianguloFora[field SFColor Black 0 0 0
					field SFColor White 0.936 0.924 0.648]{
	Transform {
		children [
			Transform {
				rotation 0 0 1 0.314
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 0.628
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 0.942
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 1.256
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 1.570
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 1.884
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 2.198
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 2.512
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 2.826
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 3.14
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 3.454
				children Triang{color IS Black}
			}

			#PARTE DE BAIXO DO CIRCULO
			Transform {
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 -0.314
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 -0.628
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 -0.942
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 -1.256
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 -1.570
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 -1.884
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 -2.198
				children Triang{color IS Black}
			}
			Transform {
				rotation 0 0 1 -2.512
				children Triang{color IS White}
			}
			Transform {
				rotation 0 0 1 -2.826
				children Triang{color IS Black}
			}
			
		]
	}
}
