#VRML V2.0 utf8

#atençao primeiro cilindro centrado na origem
PROTO torneira_urinol[]{

	Group {
		children [
			#mais junto da parede
  			Transform {
				translation	 0.02 0 0 
				children [
					Transform {
						scale 0.01 0.01 0.01
						rotation 0 0 1 -1.57
						translation	0.0125 0 0
						children[
							Shape{
								  appearance Appearance {	
									   material Material { 
											diffuseColor 0.8 0.8 0.8 
									   }
								   }
								   geometry Cylinder { 						
					    				bottom TRUE 
										height 0.5
										radius 3
										side TRUE 
										top TRUE  }
							}
						]	
					}
					#2º
					Transform {
						scale 0.01 0.01 0.01
						rotation 0 0 1 -1.57
						translation	-0.005 0 0 
						children [
							Shape{
							  appearance Appearance {	
								   material Material { 
										diffuseColor 0.8 0.8 0.8 
								   }
							   }
							   geometry Cylinder { 						
					    			bottom TRUE 
									height 4
									radius 2.5
									side TRUE 
									top TRUE  }
							}
						]
					}
					#3º
					Transform {
						scale 0.01 0.01 0.01
						rotation 0 0 1 -1.57
						translation	-0.0325 0 0
						children [
							Shape{
							  appearance Appearance {	
								   material Material { 
										diffuseColor 0.8 0.8 0.8 
								   }
							   }
							   geometry Cylinder { 						
					    			bottom TRUE 
									height 2
									radius 2
									side TRUE 
									top TRUE  }
							}
						]
					}
					#4º
					Transform {
						scale 0.01 0.01 0.01
						rotation 0 0 1 -1.57
						translation	-0.0525 0 0
						children [
							Shape{
								  appearance Appearance {	
									   material Material { 
											diffuseColor 0.8 0.8 0.8 
									   }
								   }
								   geometry Cylinder { 						
					    				bottom TRUE 
										height 2
										radius 1.5
										side TRUE 
										top TRUE  }
							}
						]
					}
					#Botão autocolismo
					DEF botao Transform {
						children [
							Transform {
								scale 0.01 0.01 0.01
								rotation 0 0 1 -1.57
								translation	-0.0725 0 0
								children [					
									Shape{
										  appearance Appearance {	
											   material Material { 
													diffuseColor 0.8 0.8 0.8 
											   }
										   }
										   geometry Cylinder { 						
					    						bottom TRUE 
												height 2
												radius 2
												side TRUE 
												top TRUE  }
									}
									DEF	TSurinol TouchSensor {
										enabled	TRUE
									}
								]
							}
						]
					}
				
					DEF	urinolTimer TimeSensor	{
						cycleInterval 3 #segundos - tempo de abrir
						loop FALSE
						startTime 0
						stopTime 0
					}

					DEF PosInterpolator PositionInterpolator {
						key      [0.0   0.5   1.0]
						keyValue [0 0 0, 0.02 0 0, 0 0 0]
					}

					Sound {
						source DEF	SomUrinol AudioClip {
						url	"Sons\urinol.wav"
						loop FALSE
					}
					}

				]
			}
			#ligação urinol

		   	Transform {
				scale 0.01 0.01 0.01
				translation	-0.0125 -0.1 0
				children [					
					Shape{
						  appearance Appearance {	
							   material Material { 
									diffuseColor 0.8 0.8 0.8 
							   }
						   }
						   geometry Cylinder { 						
					    		bottom TRUE 
								height 20
								radius 0.5
								side TRUE 
								top TRUE  }
					}
				]
			}
			#cilindro mesmo antes do urinol

		   	Transform {
				scale 0.01 0.01 0.01
				translation	-0.0125 -0.21 0
				children [					
					Shape{
						  appearance Appearance {	
							   material Material { 
									diffuseColor 0.8 0.8 0.8 
							   }
						   }
						   geometry Cylinder { 						
					    		bottom TRUE 
								height 2
								radius 1.5
								side TRUE 
								top TRUE  }
					}
				]
			}
		]
		ROUTE TSurinol.touchTime TO urinolTimer.startTime
		ROUTE urinolTimer.fraction_changed	TO PosInterpolator.set_fraction
		ROUTE PosInterpolator.value_changed TO botao.translation
		ROUTE TSurinol.touchTime TO SomUrinol.startTime
	}
}

