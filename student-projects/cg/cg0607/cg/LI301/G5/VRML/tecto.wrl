PROTO Ventilador
[
]
{
   	Transform {
	  children Shape	{
		  appearance Appearance	{
			  material Material	{ diffuseColor 1 1 1 }
			  texture	ImageTexture {
				url	"Texturas\Acessorios\ventilador.jpg"
			  }
		  }
		  geometry IndexedFaceSet	  {
			  coord Coordinate{ 
				point [ 0 0 0, 40 0 0, 40 15 0, 0 15 0, 0 0 40, 40 0 40, 40 15 40, 0 15 40,] 
			  } 
			  coordIndex [ 0 1 2 3, -1 4 5 6 7 -1, 1 2 6 5 -1, 2 6 7 3 -1, 0 3 7 4 -1, 1 5 4 0 ] 
			  color Color { color [1 1 1] } 
			  colorIndex [ 0,0,0,0,0,0,0 ]			  
			  colorPerVertex FALSE
			  solid	FALSE
		  } 		  
	  }
	}
}



