########******************************** pratos *******************************###########


PROTO prato[]
{
	Group {
		children
		[
			Shape {
				appearance  Appearance{
    			material Material {
					shininess .1
					ambientIntensity .216
					specularColor 1 1 1
					diffuseColor 1 1 1
				}
			}

				geometry Extrusion {
            		crossSection [0 0,12 -1,8 1]
					spine [	3.00	1		0.00
							2.60	1		1.50
							1.50	1	2.60
							0.00	1	3.00
							-1.50	1	2.60
							-2.60	1	1.50
							-3.00	1	0.00
							-2.60	1	-1.50
							-1.50	1	-2.60
							0.00	1	-3.00
							1.50	1	-2.60
							2.60	1	-1.50
							3.00	1	0.00
						]
					solid FALSE
					beginCap FALSE
#           	scale [1 1, 1 1, 0.5 1, 1 1]
#				orientation	[0 1 0 0.7]
        		}
			}

			Transform {
				translation	0 0.95 0
				children
				Shape{
					appearance  Appearance{
    					material Material {
        					shininess .1
							ambientIntensity .216
							specularColor 1 1 1
							diffuseColor 1 1 1
						}
					}
					geometry Cylinder {height 0.1 radius 3 }
				}
			}
		]
	}
}


PROTO comida[]
{
	Transform {
		children
		[ 

			prato {}

			Transform
			{
				translation	0 0.5 0
				children
				Shape {
						appearance Appearance {
							texture ImageTexture { url "texturas\saladaRussa2.jpg"}
#							textureTransform TextureTransform {
#								center 0 0
#								scale 5 5
#							}
						}
						geometry IndexedFaceSet {
    						coord Coordinate {
        					point [
        							-8 2 8, -8 2 -8, 8 2 -8, 8 2 8
									-9.5 1 9.5, -9.5 1 -9.5, 9.5 1 -9.5, 9.5 1 9.5
        							]
    						}
    						coordIndex [
    						0 1 2 3 -1
							0 1 5 4 -1
							2 3 7 6 -1
							7 6 5 4 -1  
    						]
    						solid FALSE
						}
					}
				}
		]
		}
}

