########******************************** copos *******************************###########
PROTO copo
[  
]
{

	Group
	{
		children [
		

		

		DEF copo_2 Shape {
			appearance Appearance{
    		material Material {
        		diffuseColor 0.8 0.8 0.8
				shininess .23
				transparency .68
				specularColor .94 .94 .94
				#emissiveColor 1 1 1
				ambientIntensity 0
    		}
		}
   
			geometry Extrusion {
            	crossSection [0 0,0.4 0,0.4 6,0 6, 0 0]
				spine [	3.00	0.50	0.00
					2.60	0.50	1.50
					1.50	0.50	2.60
					0.00	0.50	3.00
					-1.50	0.50	2.60
					-2.60	0.50	1.50
					-3.00	0.50	0.00
					-2.60	0.50	-1.50
					-1.50	0.50	-2.60
					0.00	0.50	-3.00
					1.50	0.50	-2.60
					2.60	0.50	-1.50
					3.00	0.50	0.00
					]
					solid FALSE
        	}
		}
		
		Transform {
			translation	0 -5 0
			scale 0.9 1 0.9
			children USE copo_2
		}
		Transform {
			translation	0 -10.5 0
			children
			Shape{
				appearance Appearance{
    			material Material {
        		diffuseColor 0.8 0.8 0.8
				shininess .23
				transparency .3
				specularColor .94 .94 .94
				#emissiveColor 1 1 1
				ambientIntensity 0
    		}
		}
				geometry Cylinder {height 0.1 radius 2.7}
			}
		}

	
		
		]
	}
}

#####*******************Mesas e cadeiras*****************************************

PROTO pernaCadeira
[
]
{
	Shape {	
			appearance Appearance {
    			material Material {
        			diffuseColor 0.8 0.65 0.4
    			}
			}
			geometry Box	{size 2 50 2}
	}  
}

PROTO encosto
[]
{
	Shape {#cima da cadeira
		appearance Appearance {
    		material Material {
        		diffuseColor 0.8 0.65 0.4
    		}
		}
		geometry Box	{size 48 30 1}
				
	}
}

PROTO cadeira
[
]
{
	Transform {
		children
		[
		Transform {
				translation	0 40 0
				children
				encosto	{}
		}

		
		Transform {
				
			children
			[
				Transform {	#inicio  do suporte de cadeira: lado direito
					translation	8 15 1
					scale 1 0.7 1
					children pernaCadeira{}
				}#fim do suporte de cadeira: lado direito
		   
				Transform {	 #inicio de suporte de cadeira: lado esquerdo
					translation	-7 15 1
					scale 1 0.7 1
					children pernaCadeira{}
				} #fim de suporte de cadeira: lado esquerdo
				
				Transform {
					children
					[
						Transform {
							translation	-22 -25 -1
							children
								pernaCadeira{}
						}
						Transform {
							translation	-22 -25 -35
							children
								pernaCadeira{}
						}
						Transform {
							translation	22 -25 -1
							children
								pernaCadeira{}
						}
						Transform {
							translation	22 -25 -35
							children
								pernaCadeira{}
						}

						Transform {	 #inicio de base da cadeira
							translation	0 0 -18
							rotation 1 0 0 1.57
							children
							Shape {
								appearance Appearance {
    								material Material {
        								diffuseColor 0.8 0.65 0.4
    								}
								}
								geometry Box	{size 50 40 2}
							}	
						}#fim de base da cadeira
					]
				}
			] 
		}
		
		
	]#fim da transformação de raiz
}

}

