PROTO Mesa 
[
]
{
  Transform {
    children 
    [
      Transform {
       
        children
		[
		    Tampo { }
			Transform {	 #apenas para fazer a textura por cima
				translation	0 5.01 0
				children
					Shape {
						appearance Appearance {
							texture ImageTexture { url "texturas\tmesa.jpg"
							}
						}
						geometry IndexedFaceSet {
    						coord Coordinate {
        					point [
        							-60 0 40, -60 0 -40,	60 0 -40, 60 0 40
        							]
    						}
    						coordIndex [
    						0 1 2 3 -1  
    						]
    						solid FALSE
						}
					}

			}
		]
      }
      Transform {
        translation -57.5 -35 35
        children Perna2 { }
      }
      Transform {
        translation 57.5 -35 35
        children Perna2 { }
      }
      Transform {
        translation 57.5 -35 -35
        children Perna2 { }
      }
      Transform {
        translation -57.5 -35 -35
        children Perna2 { }
      }
    ]
  }
}

PROTO Tabuleiro2[]
{

	Transform 
	{
		children
		[
		    Transform {children Tabuleiro {}}

			Transform {
				translation	0 0 0
				children comida2 {}
			}
			Transform {
				translation	12 11.51 13
				children copo{}
			}
		]
	}
}

PROTO Tabuleiro3[]
{

	Transform 
	{
		children
		[
		    Transform {children Tabuleiro {}}

			Transform {
				translation	0 0 0
				children comida {}
			}
			Transform {
				translation	12 11.51 13
				children copo{}
			}
		]
	}
}


PROTO mesaPosta[]
{
	Transform
	{
		scale 0.01 0.01 0.01
		children
		[
	
		Transform	
		{
			rotation 0 1 0 1.57
			translation	29 5 20
			children Tabuleiro2{}
		}

		Transform	
		{
			rotation 0 1 0 -1.57
			translation	29 5 -20
			children Tabuleiro2{}
		}

		Transform	
		{
			rotation 0 1 0 1.57
			translation	-29 5 20
			children Tabuleiro3{}
		}

		Transform	{
			rotation 0 1 0 -1.57
			translation	-29 5 -20
			children Tabuleiro3{}
		}

		Transform 
		{
			children
			[	
			Transform 
			{
			translation	30 -15 -50
			rotation 0 1 0 3.14
			children cadeira {}
			}

			Transform 
			{
				translation	-30 -15 -50
				rotation 0 1 0 3.14
				children cadeira {}
			}
			
			Transform
			{
				translation	-30 -15 50
				children 
				cadeira {}
			}

			Transform
			{
				translation	30 -15 50
				children 
				cadeira {}
			}
			
			Mesa { }	
			]	
		}
	]
	}

}

PROTO fila3xmesa []
{
	Transform {
		scale 0.8 0.8 0.8
		children
		[
			mesaPosta {}
			Transform
			{
				translation	1.22 0 0
				children
				mesaPosta {}
			}
			Transform
			{
				translation	-1.22 0 0
				children
				mesaPosta {}
			}

#			Axis {}
		]
	}

}


PROTO mesasdeLado []
{
	Transform 
			{	
				rotation 0 1 0 1.57
				children fila3xmesa{}}
}




