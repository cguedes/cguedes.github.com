PROTO Cone_T [	field SFVec3f position 0 0 0
				field SFFloat raio 1
				field SFFloat altura 1
				field SFRotation rotacao 0 0 0 0]{
	Transform {
		translation	IS position
		rotation  IS rotacao
		children
		Shape {
			geometry Cone{ bottomRadius IS raio height IS altura }
			appearance Appearance {
				material Material {
					diffuseColor 1 1 1
				}
			}
		}
	}
}




