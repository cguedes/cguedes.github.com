PROTO TennisFieldMarkings []
{
Transform {
	children [
			#Marcacao perto
			Transform {
				rotation 0 1 0 0
				scale 10.97 1 0.08
				translation	0 0.03 11.89
				children [					
					vFieldMarks  {}
						]
			}
			#Marcacao lado direito fora
			Transform  {
				rotation 0 1 0 1.57
				scale 23.78 1 0.08
				translation	5.485 0 0
				children [					
					vFieldMarks  {}
						]
			}
			#Marcacao longe
			Transform {
				rotation 0 1 0 0
				scale 10.97 1 0.08
				translation	0 0.03 -11.89
				children [					
					vFieldMarks  {}
						]
			}
			#Marcacao lado esquerdo fora
			Transform  {
				rotation 0 1 0 1.57
				scale 23.78 1 0.08
				translation	-5.485 0 0
				children [					
					vFieldMarks  {}
						]
			}
			#Marcacao interior esquerdo perto linha
			Transform  {
				rotation 0 1 0 1.57
				scale 23.78 1 0.08
				translation	-4.115 0.03 0
				children [					
					vFieldMarks  {}
						]
			}

			#Marcacao interior direito perto linha
			Transform  {
				rotation 0 1 0 1.57
				scale 23.78 1 0.08
				translation	4.115 0.03 0
				children [					
					vFieldMarks  {}
						]
			}

			#Marcacao centro
			Transform {
				rotation 0 1 0 0
				scale 10.97 1 0.08
				translation	0 0.03 0
				children [					
					vFieldMarks  {}
						]
			}

			#Marcacao perto centro
			Transform {
				rotation 0 1 0 0
				scale 8.23 1 0.08
				translation	0 0.03 6.4
				children [					
					vFieldMarks  {}
						]
			}
			#Marcacao longe centro
			Transform {
				rotation 0 1 0 0
				scale 8.23 1 0.08
				translation	0 0.03 -6.4
				children [					
					vFieldMarks  {}
						]
			}

			#Marcacao centro perto-fundo
			Transform  {
				rotation 0 1 0 1.57
				scale 12.8 1 0.08
				translation	0 0 0
				children [					
					vFieldMarks  {}
						]
			}

			#Pequena marcacao centro perto
			Transform  {
				rotation 0 1 0 1.57
				scale 0.5 1 0.08
				translation	0 0 11.65
				children [					
					vFieldMarks  {}
						]
			}

			#Pequena marcacao centro perto
			Transform  {
				rotation 0 1 0 1.57
				scale 0.5 1 0.08
				translation	0 0 -11.65
				children [					
					vFieldMarks  {}
						]
			}
]

}
}

PROTO TennisFieldFence [
]
{
		Transform {
		children [
		#direito  1
			Transform {		
				rotation	0 1 0 1.57	
				translation	6 0.05 4.5
				children NotSoBigFence	{}
			}
		#direito fundo
			Transform {				
				translation	6 0.05 -10.5
				rotation 0 1 0 1.57
				children [		
					HalfFence4M {}
				]
			}		
		


		#junção direito fundo
			Transform {				
				translation	6 0.05 -11
				rotation 0 1 0 1.57
				children [		
					Fence1_5MBy4MCover {}
				]
			}	
		#junção fundo direito
			Transform {				
				translation	6 0.05 -12.5
				rotation 0 1 0 -3.1415
				children [		
					Fence1_5MBy4MCover {}
				]
			}	

		#fundo direito
			Transform {				
				translation	4.5 0.05 -12.5
				rotation 0 1 0 -3.1415
				children [		
					BigFence {}
				]
			}	
			
		#fundo direito 2
			Transform {				
				translation	1.5 0.05 -12.5
				rotation 0 1 0 -3.1415
				children [		
					BigFence {}
				]
			}	
			
		#fundo direito 3
			Transform {				
				translation	-1.5 0.05 -12.5
				rotation 0 1 0 -3.1415
				children [		
					BigFence {}
				]
			}			


		#junção fundo esquerdo
			Transform {				
				translation	-4.5 0.05 -12.5
				rotation 0 1 0 -3.1415
				children [		
					Fence1_5MBy4MCover {}
				]
			}		
		#junção esquerdo fundo 2
			Transform {				
				translation	-6 0.05 -12.5
				rotation 0 1 0 -1.57
				children [		
					Fence1_5MBy4MCover {}
				]
			}	
		
		#esquerdo fundo
	   		Transform {		
				rotation	0 1 0 1.57	
				translation	-6 0.05 -10.5
				children HalfFence4MWithSidePost	{}
			}

			 
		#esquerdo 1
	   		Transform {		
				rotation	0 1 0 1.57	
				translation	-6 0.05 7.5
				children VeryBigFence	{}
			}

		#esquerdo 2
	   		Transform {		
				rotation	0 1 0 1.57	
				translation	-6 0.05 10.5
				children BigFence	{}
			}



		#junção perto esquerdo
		Transform {		
				rotation	0 1 0 1.57	
				translation	-6 0.05 12.5
				children Fence2MBy4MCover	{}
			}
		#junção perto 
		Transform {		
				rotation	0 1 0 0	
				translation	-6 0.05 12.5
				children Fence2MBy4MCover	{}
			}

		#perto 1
	   		Transform {		
				rotation	0 1 0 0	
				translation	-4 0.05 12.5
				children BigFence	{}
			}
		#perto 2
	   		Transform {		
				rotation	0 1 0 0	
				translation	-1 0.05 12.5
				children BigFence	{}
			}		
		#junção perto direita
	   		Transform {		
				rotation	0 1 0 0	
				translation	4 0.05 12.5
				children Fence2MBy4MCover	{}
			}	

		#junção perto direita
	   		Transform {		
				rotation	0 1 0 0	
				translation	2 0.05 12.5
				children Fence2MBy4MNoCover	{}
			}	
			
		#junção direita
	   		Transform {		
				rotation	0 1 0 1.57	
				translation	6 0.05 12.5
				children Fence2MBy4MCover	{}
			}

		#antes porta
	   		Transform {		
				rotation	0 1 0 1.57	
				translation	6 0.05 10.5
				children Fence2MBy4MCover	{}
			}				
		
		#Porta Grande
			Transform {				
				translation	6 1.05 10.3
				scale 1.9 2 2
				rotation 0 1 0 1.57
				children [	
					GrandePorta	{}					
				]
			}

			#Rede em cima da porta grande
			Transform {				
				translation	6 2.05 8.5
				rotation 0 1 0 1.57
				children [		
					Fence2MBy2M {}
				]
			}
			#após porta grande
			Transform {				
				translation	6 0.05 6.5
				rotation 0 1 0 1.57
				children [		
					Fence2MBy4MCover {}
				]
			}

			#Rede de ténis
			Transform {	
			translation	0 0.7 0	
			scale	1.05 1 1
				children [		
					TennisNetGroup {}
				]
			}


		]
	}
			

}

PROTO Tennis []
{
	Transform {
		children [
			TennisField {}

			TennisFieldMarkings {}

			TennisFieldWall {}

			TennisFieldFence {}
		]
	}
}


Tennis {}
