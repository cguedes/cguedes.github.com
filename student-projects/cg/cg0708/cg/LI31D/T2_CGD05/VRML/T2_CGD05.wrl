#####################################################################################
#  Papelaria AEISEL																    #
#####################################################################################


ESTRUTURA {}
Transform{
	translation	4.6 1.55 9.21
	children Vitrine{}
}


Group{
	children [
		DEF _2EstantesComAviso Transform {
			rotation 0 1 0 1.57				 # 0.36
			translation	0.721 0 8.82
			children [
				Transform{
					children [ Aviso{} Estante{}]
				}
				Transform{
					translation	1.05 0 0
					children [ Aviso{} Estante{}]
				}
			]
		}
		Transform{
			translation	0 0 -2.1
			children USE _2EstantesComAviso
		}
		Transform{
			rotation 0 1 0 1.57		  
			translation	-5.7 0 9.8
			children USE _2EstantesComAviso
		}

		DEF _2Estantes Transform {
			translation	1 0 0.6
			children [
				Transform{
					children [Estante{}]
				}
				Transform{
					translation	1.05 0 0
					children [Estante{}]
				}
			]
		}
		Transform{
			rotation 0 1 0 1.57
			translation	0 0 4
			children USE _2Estantes
		}						 
		Transform{
			rotation 0 1 0 1.57
			translation	0.72 0 4.62
			children [ Aviso{} Estante{}]
		}
	]
}
Transform {						 
	translation	3.395 0 3.57
	children movel	{}
}
Transform {						 
