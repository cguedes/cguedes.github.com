	children [
		TV{}
		Group {
			children[

#####################################################################################
#  Animação de Ligar a Televisão. Foi colocado um som ao carregar no botao          #
#####################################################################################
				Transform {
				   scale 0.02 0.02 0.02
					rotation 1 0 0 1.57
					translation	0.18 -0.184 -0.16 
					children[
			
   						Shape {
							geometry Cylinder { 
								height 0.1
								side TRUE
								top	TRUE
								bottom TRUE
								radius	0.3
							}
							appearance Appearance {
								material  Material {
									diffuseColor 0 0  0
								}												
							}
						}
						DEF TS TouchSensor { }
					]
				}				
				Sound {        
					source DEF Som AudioClip {url "Sons\TV.wav"}
				}
			]
			ROUTE TS.touchTime TO Som.startTime
#####################################################################################
		} 
	]
}

									

