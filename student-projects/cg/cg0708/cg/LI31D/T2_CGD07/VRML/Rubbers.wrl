#VRML V2.0 utf8

PROTO Rubber[
field SFVec3f translationRubber 0 0 0
field SFVec3f scaleRubber 1 1 1]{
	Transform{
		translation	IS translationRubber
		scale IS scaleRubber
		children [
			Shape {
				geometry Sphere	{radius	1}
				appearance Appearance {
					material Material {
						diffuseColor 0.1 0.1 0.1
					}
				}
			}
		]
	}	
}